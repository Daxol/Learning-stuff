package zadanie3;

import java.util.Random;

public class Macierz {

	public static void main(String[] args){
		Random rand = new Random();
		int temp;
		for (int i = 0; i < 5; i++) {
			for (int j = 0; j < 5; j++) {
				System.out.printf("%5d ",rand.nextInt(11)-5);
			}
			System.out.println();
		}
	}
}
