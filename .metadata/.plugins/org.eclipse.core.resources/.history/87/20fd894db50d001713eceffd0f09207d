static void dzielniki(){
		boolean a[][] = new boolean[10][10];
		
		for( int i = 0;i < 10;i++ ){
			for( int j = 0;j < 10;j++ ){
				if( majawspolnedzielniki(i+1,j+1) ){
					a[ i ][ j ] = true;
				}
				else
					a[ i ][ j ] = false; //chyba domyslnie jest false wiec nie trzeba tego
			}
		}
		//znowu dwie petle tlyko if( a[ i ][j ] ) print plus, else print kropka
	}