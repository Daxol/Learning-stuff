package zadanie1;

import java.util.Random;

public class RandomOneHundred {

	public static void main(String[] args) {

		Random rand = new Random();
		int temp;
		int counterEven = 0;
		int counterOdd = 0;

		int sumEven = 0;
		int sumOdd = 0;

		int maxOne = -1;
		int maxTwo = -1;
		int maxThree = -1;
		
		
		for (int i = 0; i < 100; i++) {
			temp = rand.nextInt(100-200+1)+100;
			System.out.println(temp);
		}

	}

}
