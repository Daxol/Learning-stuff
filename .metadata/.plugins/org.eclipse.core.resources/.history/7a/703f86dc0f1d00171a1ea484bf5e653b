package strona1.zad6;


public class Square {

	public static void main(String[] args){

		
		for (int i = 0; i < 10; i++) {
			for (int j = 0; j < 10; j++) {
				
				if(i==0||i==9){
					System.out.print("*");
				}
				else if(j==0||j==9){
					System.out.print("*");
				}
				else if(j==i){
					System.out.print("*");
				}
				else if(10-j==i){
					System.out.print("*");
				}
				else{
					System.out.print(" ");
				}
				
				
				
			}
			System.out.println();
		}
	}
}
