package strona3.zad3;

public class DowolnyWz√≥r {
	public static void main(String[] args){
		int[][] myArray = new int[10][10];
		try{
		for (int i = 0; i < myArray.length; i++) {
			for (int j = 0; j < myArray.length; j++) {
				myArray[i][j]=i*2+j;
				System.out.printf(" %2.d",myArray[i][j]);
			}
			System.out.println();
		}}catch(Exception e){
			e.printStackTrace();
		}
		
		
	}
}
