package strona3.zad6;

public class MyClass {

	public static void main(String[] args){
		String[] myArray = {"Daniel","Weronika","Kacper","Bartek","Oliwia",};
		for (int i = 0; i < myArray.length; i++) {
			if(myArray[i].lastIndexOf(myArray[i])=='a'||myArray[i].contains("b")){
				System.out.println(myArray[i]);
			}
		}
		for (String string : myArray) {
			if(string.lastIndexOf(string)=='a'||string.contains("b")){
				System.out.println(string);
			}
		}
	}
}
