package zadanieB1;

import java.util.Scanner;

public class Temperatura {
	
	public static void main(String[] args){
		
		Scanner in = new Scanner(System.in);
		int celsjusz;
		int faren;
		
		celsjusz = in.nextInt();
		faren = celsjusz *(9/5) +32;
		if(celsjusz<0){
			System.out.format("%c zimno",faren);
		}
		else if(celsjusz<10&& celsjusz>=0){
			System.out.println("chłodno");
		}
		else if(celsjusz<25&& celsjusz>=10){
			System.out.println("ciepło");
		}
		else if( celsjusz>=25){
			System.out.println("gorąco");
		}
	}
}
