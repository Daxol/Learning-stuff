package zadanie9;

import java.util.Scanner;

public class Macierz {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		System.out.print("Podaj rozmiar macierzy: ");
		int size = in.nextInt();
		boolean[][] arrayBool = new boolean[size][size];

	}

	public static boolean cos(int number, int number2) {
		int primeNumber = -1;
		int primeNumber2 = 0;
		for (int i = 2; i < number; i++) {
			if (number != i && number + 1 % i == 0) {
				primeNumber = i;
			}
			if (number2 != i && number2 + 1 % i == 0) {
				primeNumber2 = i;
			}
			if (primeNumber == primeNumber2)
				return true;
		}
		return false;
	}
}
